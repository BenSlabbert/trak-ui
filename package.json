{
  "name": "trak-ui",
  "version": "1.0.2",
  "private": true,
  "description": "Trak UI",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "config": {
    "husky": {
      "hooks": {
        "pre-commit": "pretty-quick --staged"
      }
    }
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "export NODE_ENV=test || SET NODE_ENV=test && mocha test/**/*.test.js",
    "test-watch": "nodemon --exec \"npm test\""
  },
  "author": "https://github/BenSlabbert",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.278.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bwip-js": "^1.7.0",
    "concurrently": "^3.6.0",
    "cookie-session": "^2.0.0-beta.3",
    "cryptiles": "^4.1.2",
    "express": "^4.16.2",
    "google-protobuf": "^3.6.1",
    "grpc": "^1.17.0",
    "hoek": "^4.1.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "npm": "^5.10.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "path-parser": "^4.2.0",
    "prop-types": "^15.6.2",
    "sync-exec": "^0.6.2",
    "trak-gRPC": "git://github.com/BenSlabbert/trak-gRPC#029ef649f81a6a1e8db4e2fa1741dc4539642983",
    "uuid": "^3.3.2",
    "winston": "^2.4.3",
    "winston-mongodb": "^3.0.2"
  },
  "devDependencies": {
    "grpc-tools": "^1.6.6",
    "husky": "^1.3.1",
    "nodemon": "^1.18.4",
    "prettier": "1.17.0",
    "pretty-quick": "^1.10.0"
  }
}
